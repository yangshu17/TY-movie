.name{
  padding-left: 12rpx;
  width: 290rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.movie-name{
  color: #58adf3
}
.m-money{
  width: 150rpx;
  text-align: center;
