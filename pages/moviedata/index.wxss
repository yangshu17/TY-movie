.c-top{
  background: #3f3f4D;
  display: flex;
  color: #fff;
  justify-content: space-around;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
}
.t-date{
  font-weight: bold;
  font-size: 28rpx;
}
.t-today{
  font-size: 34rpx;
}
.mount{
  font-weight: bold;
  color: #fc6a21
}
.t-now{
  font-weight: bold;
  font-size: 28rpx;
}
.content{
  display: flex;
  flex-direction: column;
  background: #373742;
  color: #fff;
  font-size: 28rpx;
}
.c-title{
  width: 100%;
  height: 100rpx;
